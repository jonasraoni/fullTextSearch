msgid "plugins.generic.fullTextSearch.name"
msgstr "Fuldtekst Søge Plugin"

msgid "plugins.generic.fullTextSearch.description"
msgstr "Giver en database-understøttet fuldtekst indeks for OJS indsendelser, kompatibel med MySQL og PostgreSQL."

msgid "plugins.generic.fullTextSearch.settings.selectContextsDescription"
msgstr "Vælg hvilke kontekster der skal have deres søgeindeks genopbygget."

msgid "plugins.generic.fullTextSearch.settings.rebuildButton"
msgstr "Genopbyg Søgeindeks"

msgid "plugins.generic.fullTextSearch.rebuildComplete"
msgstr "Genopbygning af søgeindekset blev gennemført med succes for de valgte kontekster."

msgid "plugins.generic.fullTextSearch.settings.clearStandardSearch"
msgstr "Ryd Standard Søgetabeller"

msgid "plugins.generic.fullTextSearch.settings.clearStandardSearchDescription"
msgstr "Valgfrit ryd standard OJS søgetabeller før genopbygning. Dette vil fjerne alle eksisterende søgedata fra standardsøgesystemet."

msgid "plugins.generic.fullTextSearch.settings.clearStandardSearchLabel"
msgstr "Ryd standard søgetabeller før genopbygning af indeks"

msgid "plugins.generic.fullTextSearch.search.error"
msgstr "Der opstod en uventet fejl under søgningen"
