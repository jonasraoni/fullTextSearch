msgid "plugins.generic.fullTextSearch.name"
msgstr "Teljes Szöveg Keresés Plugin"

msgid "plugins.generic.fullTextSearch.description"
msgstr "Adatbázis-alapú teljes szöveg indexet biztosít OJS beadványokhoz, kompatibilis MySQL-lel és PostgreSQL-lel."

msgid "plugins.generic.fullTextSearch.settings.selectContextsDescription"
msgstr "Válassza ki, mely kontextusoknak kell újraépíteni a keresési indexüket."

msgid "plugins.generic.fullTextSearch.settings.rebuildButton"
msgstr "Keresési Index Újraépítése"

msgid "plugins.generic.fullTextSearch.rebuildComplete"
msgstr "A keresési index újraépítése sikeresen befejeződött a kiválasztott kontextusokhoz."

msgid "plugins.generic.fullTextSearch.settings.clearStandardSearch"
msgstr "Szabványos Keresési Táblák Törlése"

msgid "plugins.generic.fullTextSearch.settings.clearStandardSearchDescription"
msgstr "Opcionálisan törölje a szabványos OJS keresési táblákat az újraépítés előtt. Ez eltávolítja az összes meglévő keresési adatot a szabványos keresési rendszerből."

msgid "plugins.generic.fullTextSearch.settings.clearStandardSearchLabel"
msgstr "Szabványos keresési táblák törlése az index újraépítése előtt"

msgid "plugins.generic.fullTextSearch.search.error"
msgstr "Váratlan hiba történt a keresés során"
