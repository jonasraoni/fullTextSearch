msgid "plugins.generic.fullTextSearch.name"
msgstr "Volltext-Such-Plugin"

msgid "plugins.generic.fullTextSearch.description"
msgstr "Bietet einen datenbankgestützten Volltext-Index für OJS-Einreichungen, kompatibel mit MySQL und PostgreSQL."

msgid "plugins.generic.fullTextSearch.settings.selectContextsDescription"
msgstr "Wählen Sie aus, welche Kontexte ihren Suchindex neu aufbauen sollen."

msgid "plugins.generic.fullTextSearch.settings.rebuildButton"
msgstr "Suchindex Neu Aufbauen"

msgid "plugins.generic.fullTextSearch.rebuildComplete"
msgstr "Der Neuaufbau des Suchindex wurde erfolgreich für die ausgewählten Kontexte abgeschlossen."

msgid "plugins.generic.fullTextSearch.settings.clearStandardSearch"
msgstr "Standard-Suchtabellen Löschen"

msgid "plugins.generic.fullTextSearch.settings.clearStandardSearchDescription"
msgstr "Optional die Standard-OJS-Suchtabellen vor dem Neuaufbau löschen. Dies entfernt alle vorhandenen Suchdaten aus dem Standard-Suchsystem."

msgid "plugins.generic.fullTextSearch.settings.clearStandardSearchLabel"
msgstr "Standard-Suchtabellen vor dem Neuaufbau des Index löschen"

msgid "plugins.generic.fullTextSearch.search.error"
msgstr "Bei der Suche ist ein unerwarteter Fehler aufgetreten"
