msgid "plugins.generic.fullTextSearch.name"
msgstr "Fulltextsökningsplugin"

msgid "plugins.generic.fullTextSearch.description"
msgstr "Tillhandahåller ett databasbaserat fulltextindex för OJS-inlämningar, kompatibelt med MySQL och PostgreSQL."

msgid "plugins.generic.fullTextSearch.settings.selectContextsDescription"
msgstr "Välj vilka kontexter som ska ha sitt sökindex ombyggt."

msgid "plugins.generic.fullTextSearch.settings.rebuildButton"
msgstr "Bygg Om Sökindex"

msgid "plugins.generic.fullTextSearch.rebuildComplete"
msgstr "Ombyggnad av sökindexet slutfördes framgångsrikt för de valda kontexterna."

msgid "plugins.generic.fullTextSearch.settings.clearStandardSearch"
msgstr "Rensa Standard Söktabeller"

msgid "plugins.generic.fullTextSearch.settings.clearStandardSearchDescription"
msgstr "Valfritt rensa standard OJS-söktabeller innan ombyggnad. Detta kommer att ta bort all befintlig sökdata från standardsöksystemet."

msgid "plugins.generic.fullTextSearch.settings.clearStandardSearchLabel"
msgstr "Rensa standardsöktabeller innan ombyggnad av index"

msgid "plugins.generic.fullTextSearch.search.error"
msgstr "Ett oväntat fel uppstod under sökningen"
