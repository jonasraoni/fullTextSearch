msgid "plugins.generic.fullTextSearch.name"
msgstr "Ամբողջական Տեքստի Որոնման Պլագին"

msgid "plugins.generic.fullTextSearch.description"
msgstr "Տրամադրում է տվյալների բազայի վրա հիմնված ամբողջական տեքստի ինդեքս OJS ներկայացումների համար, համատեղելի MySQL և PostgreSQL-ի հետ:"

msgid "plugins.generic.fullTextSearch.settings.selectContextsDescription"
msgstr "Ընտրեք, թե որ համատեքստերը պետք է ունենան վերակառուցված որոնման ինդեքս:"

msgid "plugins.generic.fullTextSearch.settings.rebuildButton"
msgstr "Վերակառուցել Որոնման Ինդեքսը"

msgid "plugins.generic.fullTextSearch.rebuildComplete"
msgstr "Որոնման ինդեքսի վերակառուցումը հաջողությամբ ավարտվեց ընտրված համատեքստերի համար:"

msgid "plugins.generic.fullTextSearch.settings.clearStandardSearch"
msgstr "Մաքրել Ստանդարտ Որոնման Աղյուսակները"

msgid "plugins.generic.fullTextSearch.settings.clearStandardSearchDescription"
msgstr "Ըստ ցանկության մաքրեք ստանդարտ OJS որոնման աղյուսակները վերակառուցումից առաջ: Սա կհեռացնի բոլոր գոյություն ունեցող որոնման տվյալները ստանդարտ որոնման համակարգից:"

msgid "plugins.generic.fullTextSearch.settings.clearStandardSearchLabel"
msgstr "Մաքրել ստանդարտ որոնման աղյուսակները ինդեքսի վերակառուցումից առաջ"

msgid "plugins.generic.fullTextSearch.search.error"
msgstr "Որոնման ընթացքում տեղի ունեցավ անսպասելի սխալ"
