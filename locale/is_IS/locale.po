msgid "plugins.generic.fullTextSearch.name"
msgstr "Fulltextsleit Forritsauki"

msgid "plugins.generic.fullTextSearch.description"
msgstr "Veitir gagnagrunnsundirstöðu fulltextsleit fyrir OJS framlög, samhæft við MySQL og PostgreSQL."

msgid "plugins.generic.fullTextSearch.settings.selectContextsDescription"
msgstr "Veldu hvaða samhengi ættu að hafa leitarvísitölu sína endurbyggða."

msgid "plugins.generic.fullTextSearch.settings.rebuildButton"
msgstr "Endurbyggja Leitarvísitölu"

msgid "plugins.generic.fullTextSearch.rebuildComplete"
msgstr "Endurbygging leitarvísitölunnar tókst fyrir valin samhengi."

msgid "plugins.generic.fullTextSearch.settings.clearStandardSearch"
msgstr "Hreinsa Staðlaðar Leitarfærslur"

msgid "plugins.generic.fullTextSearch.settings.clearStandardSearchDescription"
msgstr "Valfrjálst hreinsa staðlaðar OJS leitarfærslur fyrir endurbygginguna. Þetta mun fjarlægja allar fyrirliggjandi leitargögn úr staðlaða leitarkerfinu."

msgid "plugins.generic.fullTextSearch.settings.clearStandardSearchLabel"
msgstr "Hreinsa staðlaðar leitarfærslur fyrir endurbygginguna á vísitölunni"

msgid "plugins.generic.fullTextSearch.search.error"
msgstr "Óvænt villa kom upp við leitina"
