msgid "plugins.generic.fullTextSearch.name"
msgstr "Plugin de Recherche en Texte Intégral"

msgid "plugins.generic.fullTextSearch.description"
msgstr "Fournit un index de texte intégral basé sur une base de données pour les soumissions OJS, compatible avec MySQL et PostgreSQL."

msgid "plugins.generic.fullTextSearch.settings.selectContextsDescription"
msgstr "Choisissez quels contextes doivent avoir leur index de recherche reconstruit."

msgid "plugins.generic.fullTextSearch.settings.rebuildButton"
msgstr "Reconstruire l'Index de Recherche"

msgid "plugins.generic.fullTextSearch.rebuildComplete"
msgstr "La reconstruction de l'index de recherche s'est terminée avec succès pour les contextes sélectionnés."

msgid "plugins.generic.fullTextSearch.settings.clearStandardSearch"
msgstr "Effacer les Tables de Recherche Standard"

msgid "plugins.generic.fullTextSearch.settings.clearStandardSearchDescription"
msgstr "Optionnellement effacer les tables de recherche standard d'OJS avant de reconstruire. Cela supprimera toutes les données de recherche existantes du système de recherche par défaut."

msgid "plugins.generic.fullTextSearch.settings.clearStandardSearchLabel"
msgstr "Effacer les tables de recherche standard avant de reconstruire l'index"

msgid "plugins.generic.fullTextSearch.search.error"
msgstr "Une erreur inattendue s'est produite lors de la recherche"
