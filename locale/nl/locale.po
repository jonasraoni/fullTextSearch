msgid "plugins.generic.fullTextSearch.name"
msgstr "Volledige Tekst Zoek Plugin"

msgid "plugins.generic.fullTextSearch.description"
msgstr "Biedt een database-ondersteunde volledige tekst index voor OJS inzendingen, compatibel met MySQL en PostgreSQL."

msgid "plugins.generic.fullTextSearch.settings.selectContextsDescription"
msgstr "Kies welke contexten hun zoekindex moeten laten herbouwen."

msgid "plugins.generic.fullTextSearch.settings.rebuildButton"
msgstr "Zoekindex Herbouwen"

msgid "plugins.generic.fullTextSearch.rebuildComplete"
msgstr "Het herbouwen van de zoekindex is succesvol voltooid voor de geselecteerde contexten."

msgid "plugins.generic.fullTextSearch.settings.clearStandardSearch"
msgstr "Standaard Zoektabellen Wissen"

msgid "plugins.generic.fullTextSearch.settings.clearStandardSearchDescription"
msgstr "Optioneel de standaard OJS zoektabellen wissen voordat u herbouwt. Dit verwijdert alle bestaande zoekgegevens uit het standaard zoeksysteem."

msgid "plugins.generic.fullTextSearch.settings.clearStandardSearchLabel"
msgstr "Standaard zoektabellen wissen voordat u de index herbouwt"

msgid "plugins.generic.fullTextSearch.search.error"
msgstr "Er is een onverwachte fout opgetreden tijdens het zoeken"
