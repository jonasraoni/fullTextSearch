msgid "plugins.generic.fullTextSearch.name"
msgstr "Plugin Căutare Text Complet"

msgid "plugins.generic.fullTextSearch.description"
msgstr "Oferă un index de text complet bazat pe baza de date pentru înregistrările OJS, compatibil cu MySQL și PostgreSQL."

msgid "plugins.generic.fullTextSearch.settings.selectContextsDescription"
msgstr "Alegeți ce contexte ar trebui să aibă indexul de căutare reconstruit."

msgid "plugins.generic.fullTextSearch.settings.rebuildButton"
msgstr "Reconstruiește Indexul de Căutare"

msgid "plugins.generic.fullTextSearch.rebuildComplete"
msgstr "Reconstruirea indexului de căutare a fost finalizată cu succes pentru contextele selectate."

msgid "plugins.generic.fullTextSearch.settings.clearStandardSearch"
msgstr "Șterge Tabelele de Căutare Standard"

msgid "plugins.generic.fullTextSearch.settings.clearStandardSearchDescription"
msgstr "Opțional șterge tabelele de căutare standard OJS înainte de reconstruire. Aceasta va elimina toate datele de căutare existente din sistemul de căutare standard."

msgid "plugins.generic.fullTextSearch.settings.clearStandardSearchLabel"
msgstr "Șterge tabelele de căutare standard înainte de reconstruirea indexului"

msgid "plugins.generic.fullTextSearch.search.error"
msgstr "A apărut o eroare neașteptată în timpul căutării"
