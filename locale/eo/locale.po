msgid "plugins.generic.fullTextSearch.name"
msgstr "Plagino por Plena Teksta Serĉo"

msgid "plugins.generic.fullTextSearch.description"
msgstr "Provizas datumbazan plenan tekstan indekson por OJS-enmetoj, kongrua kun MySQL kaj PostgreSQL."

msgid "plugins.generic.fullTextSearch.settings.selectContextsDescription"
msgstr "Elektu kiujn kuntekstojn devus havi sian serĉan indekson rekonstruita."

msgid "plugins.generic.fullTextSearch.settings.rebuildButton"
msgstr "Rekonstrui Serĉan Indekson"

msgid "plugins.generic.fullTextSearch.rebuildComplete"
msgstr "Serĉa indekso rekonstruo kompletigita sukcese por la elektitaj kuntekstoj."

msgid "plugins.generic.fullTextSearch.settings.clearStandardSearch"
msgstr "Malplenigi Normajn Serĉajn Tabelojn"

msgid "plugins.generic.fullTextSearch.settings.clearStandardSearchDescription"
msgstr "Laŭvole malplenigi la normajn OJS serĉajn tabelojn antaŭ rekonstruo. Ĉi tio forigos ĉiujn ekzistantajn serĉajn datumojn de la defaŭlta serĉa sistemo."

msgid "plugins.generic.fullTextSearch.settings.clearStandardSearchLabel"
msgstr "Malplenigi normajn serĉajn tabelojn antaŭ rekonstruo de indekso"

msgid "plugins.generic.fullTextSearch.search.error"
msgstr "Neatendita eraro okazis dum serĉado"